{
  "name": "AppUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "LastName": {
      "type": "string"
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "TimeCreate": {
      "type": "date"
    },
    "isTemporal": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "isMilitar": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "contactinfo": {
      "type": "belongsTo",
      "model": "Contactinfo",
      "foreignKey": "UserID",
      "options": {
        "nestRemoting": true
      }
    },
    "userdocument": {
      "type": "belongsTo",
      "model": "Userdocument",
      "foreignKey": "UserID"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
